package day04;

public class 영화정보테스트 {

	public static void main(String[] args) {
		
		영화정보 m1 = new 영화정보("신세계","홍길동",10000);
		영화정보 m2 = new 영화정보("베테랑","김길동",15000);
		
		m1.printInfo();
		m2.printInfo();
		
		System.out.println(m1.get제목()+" "+m1.get감독());
		System.out.println(m2.get제목()+" "+m2.get감독());
		
	}

}
