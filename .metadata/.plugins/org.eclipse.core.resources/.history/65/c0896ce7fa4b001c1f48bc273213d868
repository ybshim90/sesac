package day01;

public class 타입변환 {

	public static void main(String[] args) {
		int i = (int)100.1;
		System.out.println(i);
		System.out.println(1/2.0);
		
		System.out.println(1+2);
		System.out.println(1+" "+2);

		System.out.println(1+2+" "+(3+4));
		System.out.println(1*2+" "+3*4);
		
		System.out.println("5");
	}

}
