package day01;

public class 로또번호배열 {

	public static void main(String[] args) {
		int[] lottos = new int[6];
		for (int i=1; i<=6; i++) {
			int r = (int)(Math.random()*45+1);
//			System.out.println(r);
			
			lottos [i-1] = r;
		}
		for (int i=0; i<6; i++) {
			System.out.println(lottos[i]);
		}
	}
}
